[comment encoding = UTF-8 /]
[module shared('http://www.eclipse.org/uml2/5.0.0/UML')]
[import frontend::shared::mocks /]
[import frontend::shared::crud /]
[import frontend::shared::master /]
[import frontend::shared::auth /]

[template public genShared(m : Model, path : String)]
[let path : String = path.concat('src/shared/')]
	[m.crudMain(path)/]
	[m.mocksMain(path)/]
    [m.mainMaster(path)/]
    [m.mainAuth(path)/]
[/let]
[/template]

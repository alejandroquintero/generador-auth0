[comment encoding = UTF-8 /]
[module config('http://www.eclipse.org/uml2/5.0.0/UML')]
[import ::utils /]
[import backend::config::pom /]
[import backend::config::xml /]

[template public genConfig(m : Model)]
[let project : String = m.getBackendProjectName()]
    [m.genWebPOM(project.concat('/'))/]
    [m.genBeansXml(project.concat('/src/main/webapp/WEB-INF/'))/]
    [m.genWebXML(project.concat('/src/main/webapp/WEB-INF/'))/]
[/let]
[/template]
